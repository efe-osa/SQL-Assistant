{
  "name": "sql-assistant-backend",
  "version": "1.0.0",
  "description": "Backend for SQL Assistant",
  "author": "Efe-osa Osawaru Oyelade",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc --project tsconfig.json --outDir dist --rootDir src",
    "lint": "eslint --fix ./src/**/*.ts",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\"",
    "sqlite:dev": "docker build -f ./Dockerfile.sqlite -t sql-assistant-sqlite . && docker run -it sql-assistant-sqlite"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@langchain/community": "^0.0.34",
    "@langchain/core": "^0.1.63",
    "axios": "^1.9.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "langchain": "^0.1.25",
    "langsmith": "^0.3.30",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20",
    "winston": "^3.11.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/sqlite3": "^3.1.11",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  }
}
